{
    "openapi": "3.0.0",
    "info": {
      "title": "Project2 API",
      "version": "1.0.0",
      "description": "API documentation for Project2"
    },
    "servers": [
      {
        "url": "https://project2-api-gatx.onrender.com",
        "description": "Production Server"
      }
    ],
    "paths": {
      "/api/items": {
        "get": {
          "summary": "Get all items",
          "responses": {
            "200": {
              "description": "List of items",
              "content": {
                "application/json": {
                  "example": [
                    {
                      "id": "660a9b3e86c9e6001c27b001",
                      "name": "Item 1",
                      "price": 50
                    }
                  ]
                }
              }
            }
          }
        },
        "post": {
          "summary": "Create a new item",
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "example": {
                  "name": "New Item",
                  "price": 100
                }
              }
            }
          },
          "responses": {
            "201": {
              "description": "Item created",
              "content": {
                "application/json": {
                  "example": {
                    "id": "660a9b3e86c9e6001c27b002",
                    "name": "New Item",
                    "price": 100
                  }
                }
              }
            },
            "400": {
              "description": "Invalid request"
            }
          }
        }
      },
      "/api/items/{id}": {
        "get": {
          "summary": "Get item by ID",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Item found",
              "content": {
                "application/json": {
                  "example": {
                    "id": "660a9b3e86c9e6001c27b001",
                    "name": "Item 1",
                    "price": 50
                  }
                }
              }
            },
            "404": {
              "description": "Item not found"
            }
          }
        },
        "put": {
          "summary": "Update an item",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "requestBody": {
            "required": true,
            "content": {
              "application/json": {
                "example": {
                  "name": "Updated Item",
                  "price": 120
                }
              }
            }
          },
          "responses": {
            "200": {
              "description": "Item updated",
              "content": {
                "application/json": {
                  "example": {
                    "id": "660a9b3e86c9e6001c27b001",
                    "name": "Updated Item",
                    "price": 120
                  }
                }
              }
            },
            "400": {
              "description": "Invalid request"
            },
            "404": {
              "description": "Item not found"
            }
          }
        },
        "delete": {
          "summary": "Delete an item",
          "parameters": [
            {
              "name": "id",
              "in": "path",
              "required": true,
              "schema": {
                "type": "string"
              }
            }
          ],
          "responses": {
            "200": {
              "description": "Item deleted"
            },
            "404": {
              "description": "Item not found"
            }
          }
        }
      }
    }
  }
  